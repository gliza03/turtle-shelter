    <script>
        // Spinning Number Animation
        let numberElement = document.getElementById("spinning-number");
        let currentNumber = 0;
        const targetNumber = 1200;

        const incrementNumber = () => {
            if (currentNumber < targetNumber) {
                currentNumber += Math.ceil(targetNumber / 100);
                numberElement.textContent = currentNumber > targetNumber ? targetNumber : currentNumber;
                requestAnimationFrame(incrementNumber);
            }
        };

        incrementNumber();
    </script>
